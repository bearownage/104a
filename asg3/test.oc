#include "oclib.h"
#
#define EMPTY (-1)

struct stack {
   string[] data;
   int size;
   int top;
}

stack new_stack (int size) {
   stack stack = new stack; // Zeros out both fields
   stack->data = new string[size]; // Array of null pointers
   stack->size = size;
   stack->top = EMPTY;
   return stack;
}

int main() {
    assert(5);
}
